$("#object_edit_area").find("div").remove();
$("table#checkmen tr").css('background','');
$("#checkmen").find("#object_<%= @object.id%>").css('background','#66FF66');
$("#object_edit_area").append("<%= j render :partial => 'form'%>")
$("#object_edit_area").find(".close").bind(
	'click',
	function(event){
		var tableid = '#' +$(this).attr("id");
		$("#checkmen").find(tableid).css('background','');
	}
	)
