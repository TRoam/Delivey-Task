<table id="checkmen" class="display">
	<thead>
		<th>Priority</th>
		<th>Status</th>
		<th>System</th>
		<th>Check(Check_id)</th>
		<th>Checkmessage(message_id)</th>
		<th>Objectname</th>
		<th>Package</th>
		<th>Application Component</th>
		<th>Contact</th>
		<th>Responsible_Name</th>
		<th>Prodrel</th>
	    <th>Comment</th>
	</thead>
	<tbody>
		<% @checkman.each do |checkman|%>
		<tr id="checkman_<%= checkman.id%>">
		<td><%= checkman.priority %></td>
		<td>
			<span>
	    	<% if checkman.comments.empty?||checkman.comments.last.feedback=="open"%>
	    	<i class="icon-share"></i>open
			<%else%>
			<i class="icon-check"></i>solved
			<%end%>
	    	</span>
	    </td>
		<td><%= checkman.release%></td>
		<td><%= checkman.checkid%></td>
		<td><%= checkman.messageid%></td>
		<td><%= link_to checkman.objectresponsible.objectname, :controller =>:objectresponsibles,:action =>:detail,:id=>checkman.objectresponsible.id%></td>
		<td><%= link_to checkman.objectresponsible.component.package,:controller => :components,:action =>:detail,:id=>checkman.objectresponsible.component.id%></td>
		<td><%= checkman.objectresponsible.component.applicationcomponent%></td>
		<td><%= checkman.objectresponsible.contact%></td>
		<td><%= link_to checkman.objectresponsible.person.responsibleperson, :controller =>:people,:action =>:detail,:id =>checkman.objectresponsible.person.id%></td>
		<td><%if checkman.prodrel? %>
			<i class="icon-ok"></i>
			<%else%>
			<i class="icon-remove"></i>
			<%end%>
		</td>
	    <td><span><%= checkman.comments.empty?? "No Comment":checkman.comments.last.content%></span><%= link_to "", edit_checkman_path(checkman.id),:remote=>true,:class=>"icon-pencil"%></td>
		</tr>
		<%end%>
	</tbody>
</table>